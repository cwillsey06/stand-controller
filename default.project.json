{
  "name": "stand-controller",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "common": {
        "$path": "src/common",
        "$ignoreUnknownInstances": true,

        "Packages": {
          "$path": "Packages"
        },
        
        "StandService": {
          "$path": "src/common/StandService",
          "ContactServer": {
            "$className": "RemoteFunction"
          }
        }
      }
    },

    "ServerStorage": {
      "private": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      }
    },

    "ServerScriptService": {
      "server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "client": {
          "$path": "src/client"
        }
      }
    }
  }
}