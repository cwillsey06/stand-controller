{
  "name": "stand-controller",
  "tree": {
    "$className": "DataModel",

    "Workspace": {
      "$className": "Workspace",
      "$ignoreUnknownInstances": true,
      
      "PlayerStands": {
        "$className": "Folder"
      }
    },

    "ReplicatedStorage": {
      "common": {
        "$path": "src/common",
        "$ignoreUnknownInstances": true,

        "Packages": {
          "$path": "Packages"
        }
      }
    },

    "ServerStorage": {
      "private": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      }
    },

    "ServerScriptService": {
      "server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "client": {
          "$path": "src/client"
        }
      }
    }
  }
}